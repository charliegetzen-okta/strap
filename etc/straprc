#@IgnoreInspection BashAddShebang
#
# WARNING
#
# DO NOT MODIFY THIS FILE. IT MAY BE AUTOMATICALLY RE-GENERATED BY STRAP AT ANY TIME.
#
if [ -d "${HOME}/.strap/releases/current/bin" ]; then
  export PATH="${HOME}/.strap/releases/current/bin:$PATH"
fi

if ls -A "${HOME}"/.strap/etc/straprc.d/*.sh >/dev/null 2>&1; then
  for file in "${HOME}"/.strap/etc/straprc.d/*.sh; do
    source "${file}"
  done
fi
